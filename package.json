{
  "name": "hull-hubspot",
  "description": "Send Notification of User events and segments to Hubspot",
  "version": "0.3.15",
  "homepage": "https://github.com/hull-ships/hull-hubspot",
  "license": "MIT",
  "main": "bin/start",
  "author": {
    "name": "Hull",
    "email": "contact@hull.io",
    "url": "https://github.com/hull"
  },
  "engines": {
    "node": "6.12.x",
    "npm": "3.10.x",
    "yarn": "1.3.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/hull-ships/hull-hubspot"
  },
  "bugs": {
    "url": "https://github.com/hull-ships/hull-hubspot/issues"
  },
  "scripts": {
    "ngrok": "ngrok http 8082 --region eu --subdomain hubspot",
    "build": "npm run clean && npm run build:server",
    "build:server": "babel server -d build",
    "start": "COMBINED=true node build",
    "start:dev": "COMBINED=true babel-watch -L server",
    "test": "npm run test:lint && npm run test:flow && npm run test:unit && npm run test:integration && npm run test:manifest",
    "test:lint": "eslint server",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration",
    "test:flow": "flow check",
    "test:manifest": "jsonlint manifest.json -q",
    "clean": "rimraf dist; rimraf build",
    "update": "updtr",
    "postinstall": "npm run build"
  },
  "keywords": [
    "hull",
    "ship",
    "hull-ship",
    "webhooks",
    "integration",
    "user",
    "sync",
    "hubspot"
  ],
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-watch": "^2.0.2",
    "chai": "^4.0.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb-base": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "flow-bin": "^0.59.0",
    "hull-connector-tester": "^0.0.4",
    "mini-application": "^1.0.5",
    "minihull": "^2.1.0",
    "mocha": "^2.5.2",
    "sinon": "^4.0.2",
    "updtr": "^0.2.1"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.13.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.3.13",
    "bluebird": "^3.4.3",
    "cache-manager-redis": "^0.4.0",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "hull": "0.13.10",
    "jsonlint": "^1.6.2",
    "lodash": "^4.14.2",
    "moment": "^2.14.1",
    "newrelic": "^2.4.1",
    "passport-hubspot-oauth2.0": "^1.0.2",
    "promise-retry": "^1.1.1",
    "raw-body": "^2.1.7",
    "rimraf": "^2.5.4",
    "slug": "^0.9.1",
    "superagent": "^3.8.2",
    "superagent-prefix": "0.0.2"
  }
}
